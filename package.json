{
  "name": "kruize-ui",
  "version": "0.0.7",
  "repository": "https://github.com/kruize/kruize-ui.git",
  "license": "MIT",
  "private": false,
  "browser": {
    "child_process": false
  },
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8080",
    "start:dev": "webpack serve --color --progress --config webpack.dev.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src --config ./.prettierrc",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@timkendrick/monaco-editor-loader": "^0.0.7",
    "@types/enzyme": "^3.10.15",
    "@types/jest": "^26.0.23",
    "@types/react-router-dom": "^5.1.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^9.1.3",
    "babel-preset-react": "^6.1.18",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv-webpack": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.28.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^27.0.4",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.3.1",
    "prettier": "^3.0.3",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.14.0",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.9.0"
  },
  "peerDependencies": {
    "monaco-editor": "^0.21.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-monaco-editor": "^0.41.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@patternfly/patternfly": "^5.0.4",
    "@patternfly/react-charts": "^7.0.1",
    "@patternfly/react-code-editor": "^5.0.1",
    "@patternfly/react-core": "^5.0.1",
    "@patternfly/react-icons": "^5.0.1",
    "@patternfly/react-table": "^5.0.1 ",
    "cssnano": "^5.1.12",
    "cssnano-preset-lite": "^2.1.3",
    "history": "^5.3.0",
    "html-loader": "^4.2.0",
    "mathjax-react": "^2.0.1",
    "monaco-editor": "^0.21.3",
    "monaco-editor-webpack-plugin": "^2.1.0",
    "monaco-yaml": "^4.0.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-markdown": "^8.0.2",
    "react-monaco-editor": "^0.41.2",
    "react-router-last-location": "^2.0.1",
    "sirv-cli": "^1.0.12"
  }
}
